{
  "name": "react-fast-training-api",
  "version": "1.0.0",
  "description": "React Fast Training Backend API - First Aid Training Company in Yorkshire",
  "keywords": [
    "loopback-application",
    "loopback"
  ],
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "engines": {
    "node": ">=18"
  },
  "scripts": {
    "build": "tsc --project tsconfig.build.json --noEmitOnError false || echo 'Build completed with warnings'",
    "build:watch": "lb-tsc --watch",
    "lint": "npm run eslint && npm run prettier:check",
    "lint:fix": "npm run eslint:fix && npm run prettier:fix",
    "prettier:cli": "lb-prettier \"**/*.ts\" \"**/*.js\"",
    "prettier:check": "npm run prettier:cli -- -l",
    "prettier:fix": "npm run prettier:cli -- --write",
    "eslint": "lb-eslint --report-unused-disable-directives .",
    "eslint:fix": "npm run eslint -- --fix",
    "pretest": "npm run rebuild",
    "test": "lb-mocha --allow-console-logs \"dist/__tests__\"",
    "posttest": "npm run lint",
    "test:dev": "lb-mocha --allow-console-logs dist/__tests__/**/*.js && npm run posttest",
    "docker:build": "docker build -t react-fast-training-api .",
    "docker:run": "docker run -p 3000:3000 -d react-fast-training-api",
    "premigrate": "npm run build",
    "sync-stripe-products": "ts-node src/scripts/sync-stripe-products.ts",
    "migrate": "node ./dist/migrate",
    "preopenapi-spec": "npm run build",
    "openapi-spec": "node ./dist/openapi-spec",
    "start": "node index.js",
    "dev": "nodemon --watch src --ext ts --exec 'npm run build && node .'",
    "clean": "lb-clean dist *.tsbuildinfo .eslintcache",
    "rebuild": "npm run clean && npm run build",
    "seed": "node ./dist/seed-data",
    "knex": "knex",
    "migrate:latest": "npx knex migrate:latest",
    "migrate:rollback": "npx knex migrate:rollback",
    "migrate:make": "npx knex migrate:make",
    "seed:run": "npx knex seed:run",
    "db:setup": "ts-node src/database/setup.ts setup",
    "db:migrate": "ts-node src/database/setup.ts migrate",
    "db:rollback": "ts-node src/database/setup.ts rollback",
    "db:seed": "ts-node src/database/setup.ts seed",
    "db:reset": "ts-node src/database/setup.ts reset",
    "db:verify": "ts-node src/database/setup.ts verify",
    "db:test": "ts-node src/database/config.ts",
    "setup:database": "node src/scripts/setup-database.js",
    "heroku-postbuild": "npm install --legacy-peer-deps && npm run build"
  },
  "repository": {
    "type": "git",
    "url": ""
  },
  "author": "React Fast Training",
  "license": "UNLICENSED",
  "files": [
    "README.md",
    "dist",
    "src",
    "!*/__tests__"
  ],
  "dependencies": {
    "@loopback/authentication": "^8.0.0",
    "@loopback/authentication-jwt": "^0.11.0",
    "@loopback/authorization": "^0.11.0",
    "@loopback/boot": "^4.0.0",
    "@loopback/core": "^3.0.0",
    "@loopback/repository": "^4.0.0",
    "@loopback/rest": "^11.0.0",
    "@loopback/rest-explorer": "^4.0.0",
    "@loopback/service-proxy": "^4.0.0",
    "@paralleldrive/cuid2": "^2.2.2",
    "@types/validator": "^13.15.2",
    "bcryptjs": "^2.4.3",
    "cors": "^2.8.5",
    "crypto": "^1.0.1",
    "csurf": "^1.11.0",
    "date-fns": "^3.3.1",
    "dotenv": "^16.6.1",
    "drizzle-orm": "^0.30.10",
    "express-rate-limit": "^8.0.1",
    "express-session": "^1.18.2",
    "helmet": "^8.1.0",
    "jsonwebtoken": "^9.0.2",
    "knex": "^3.1.0",
    "loopback-connector-postgresql": "^7.1.4",
    "nodemailer": "^6.9.13",
    "pdfkit": "^0.17.1",
    "pg": "^8.11.3",
    "socket.io": "^4.8.1",
    "stripe": "^14.25.0",
    "tslib": "^2.6.2",
    "typescript": "~5.3.3",
    "uuid": "^9.0.1"
  },
  "devDependencies": {
    "@loopback/build": "^8.0.0",
    "@loopback/eslint-config": "^12.0.0",
    "@loopback/testlab": "^4.0.0",
    "@types/bcryptjs": "^2.4.6",
    "@types/jsonwebtoken": "^9.0.5",
    "@types/node": "^18.19.42",
    "@types/nodemailer": "^6.4.15",
    "@types/pdfkit": "^0.17.2",
    "@types/pg": "^8.10.9",
    "@types/uuid": "^9.0.8",
    "concurrently": "^9.2.0",
    "eslint": "^8.57.0",
    "nodemon": "^3.0.3",
    "source-map-support": "^0.5.21",
    "ts-node": "^10.9.2"
  }
}
